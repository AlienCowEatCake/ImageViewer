install-data-local:
	@echo "Installing doxymentation"; \
	 cd $(srcdir) && \
	 $(mkinstalldirs) $(DESTDIR)$(htmldocdir); \
	 (for i in *.html doxygen.*; do \
	    $(INSTALL_DATA) $$i $(DESTDIR)$(htmldocdir)/$$i; \
	  done)

