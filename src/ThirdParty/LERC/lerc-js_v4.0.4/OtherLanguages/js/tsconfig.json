{
  "compilerOptions": {
    "module": "ES6",
    "lib": ["ES2020", "ES2020.Promise", "DOM", "scripthost"],
    "noImplicitAny": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es6",
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "importHelpers": false,
    "moduleResolution": "node"
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": []
}