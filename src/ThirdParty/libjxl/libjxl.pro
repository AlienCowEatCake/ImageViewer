CONFIG += staticlib
TARGET = tp_libjxl

QT -= core gui

CONFIG -= warn_on
CONFIG += warn_off

THIRDPARTY_LIBJXL_PATH = $${PWD}/libjxl-0.7.0
THIRDPARTY_LIBJXL_CONFIG_PATH = $${PWD}/config

include(../CommonSettings.pri)
include(../brotli/brotli.pri)
include(../highway/highway.pri)
include(../LittleCMS2/LittleCMS2.pri)

