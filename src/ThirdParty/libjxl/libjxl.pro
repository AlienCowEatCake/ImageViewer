    $${THIRDPARTY_LIBJXL_PATH}/lib/include \
    $${THIRDPARTY_LIBJXL_PATH} \
    $${THIRDPARTY_LIBJXL_CONFIG_PATH} \
    $${INCLUDEPATH}

