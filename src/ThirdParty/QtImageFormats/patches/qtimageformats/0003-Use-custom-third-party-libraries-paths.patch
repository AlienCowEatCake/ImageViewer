From 88cea6f98946278096e3b9226abb9326881c37c9 Mon Sep 17 00:00:00 2001
From: Peter Zhigalov <peter.zhigalov@gmail.com>
Date: Sun, 14 May 2017 23:13:29 +0700
Subject: [PATCH 3/5] Use custom third-party libraries paths

---
 src/3rdparty/zlib_dependency.pri             | 11 ++++++-----
 src/plugins/imageformats/jp2/qjp2handler.pri | 12 ++++++------
 src/plugins/imageformats/mng/qmnghandler.pri | 14 +++++++-------
 3 files changed, 19 insertions(+), 18 deletions(-)

diff --git a/src/3rdparty/zlib_dependency.pri b/src/3rdparty/zlib_dependency.pri
index 2c714b3..34187a4 100644
--- a/src/3rdparty/zlib_dependency.pri
+++ b/src/3rdparty/zlib_dependency.pri
@@ -1,6 +1,7 @@
 # zlib dependency satisfied by bundled 3rd party zlib or system zlib
-qtConfig(system-zlib) {
-    QMAKE_USE_PRIVATE += zlib
-} else {
-    QT_PRIVATE += zlib-private
-}
+#qtConfig(system-zlib) {
+#    QMAKE_USE_PRIVATE += zlib
+#} else {
+#    QT_PRIVATE += zlib-private
+#}
+include($${PWD}/../../../../zlib/zlib.pri)
diff --git a/src/plugins/imageformats/jp2/qjp2handler.pri b/src/plugins/imageformats/jp2/qjp2handler.pri
index 539daaa..4edf6ba 100644
--- a/src/plugins/imageformats/jp2/qjp2handler.pri
+++ b/src/plugins/imageformats/jp2/qjp2handler.pri
@@ -2,9 +2,9 @@
 INCLUDEPATH *= $$PWD
 HEADERS += $$PWD/qjp2handler_p.h
 SOURCES += $$PWD/qjp2handler.cpp
-config_jasper {
-    msvc: LIBS += libjasper.lib
-    else: LIBS += -ljasper
-} else {
-    include($$PWD/../../../3rdparty/jasper.pri)
-}
+#config_jasper {
+#    msvc: LIBS += libjasper.lib
+#    else: LIBS += -ljasper
+#} else {
+#    include($$PWD/../../../3rdparty/jasper.pri)
+#}
diff --git a/src/plugins/imageformats/mng/qmnghandler.pri b/src/plugins/imageformats/mng/qmnghandler.pri
index 5c0fff1..25a2939 100644
--- a/src/plugins/imageformats/mng/qmnghandler.pri
+++ b/src/plugins/imageformats/mng/qmnghandler.pri
@@ -2,10 +2,10 @@
 INCLUDEPATH *= $$PWD
 HEADERS += $$PWD/qmnghandler_p.h
 SOURCES += $$PWD/qmnghandler.cpp
-config_libmng {
-    unix|mingw: LIBS += -lmng
-    else:win32: LIBS += libmng.lib
-} else {
-    include($$PWD/../../../3rdparty/libmng.pri)
-    *-g++*: QMAKE_CFLAGS_WARN_ON += -Wno-unused-parameter
-}
+#config_libmng {
+#    unix|mingw: LIBS += -lmng
+#    else:win32: LIBS += libmng.lib
+#} else {
+#    include($$PWD/../../../3rdparty/libmng.pri)
+#    *-g++*: QMAKE_CFLAGS_WARN_ON += -Wno-unused-parameter
+#}
